<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nathalieseibert.wells.Statisitk">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:gravity="center|top"
        android:text="@string/title_Statistik"
        android:textColor="@color/schwoaz"
        android:textSize="25sp"

        />

    <Button
        android:id="@+id/verlauf"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|start"
        android:layout_marginBottom="20dp"
        android:layout_marginStart="20dp"
        android:text="@string/title_Verlauf"
        android:layout_marginLeft="20dp" />

    <Button
        android:id="@+id/statistik"
        android:layout_width="160dp"
        android:layout_marginEnd="20dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:enabled="false"
        android:layout_marginBottom="20dp"
        android:text="@string/title_Statistik"
        android:layout_marginRight="20dp" />




</FrameLayout>
