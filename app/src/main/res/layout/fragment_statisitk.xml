<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    tools:context="at.fhjoanneum.gruppeWells.wells.Statisitk">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:gravity="center|top"
        android:text="@string/title_Statistik"
        android:textColor="@color/schwoaz"
        android:textSize="25sp" />


    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="20dp">

        <TextView
            android:id="@+id/brunneneins"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/haefigkeiteins"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/unbekannt"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="20dp">

        <TextView
            android:id="@+id/brunnenzwei"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/haefigkeitzwei"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/unbekannt"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="20dp">

        <TextView
            android:id="@+id/brunnendrei"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/haefigkeitdrei"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/unbekannt"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="20dp">

        <TextView
            android:id="@+id/brunnenvier"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/haefigkeitvier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/unbekannt"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="20dp">

        <TextView
            android:id="@+id/brunnenfunf"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/haefigkeitfunf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/unbekannt"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </TableRow>

    <Button
        android:id="@+id/verlauf"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|start"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginStart="20dp"
        android:gravity="center"
        android:text="@string/title_Verlauf" />


    <Button
        android:id="@+id/statistik"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:enabled="false"
        android:gravity="center"
        android:text="@string/title_Statistik" />

</TableLayout>
